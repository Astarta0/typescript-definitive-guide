#!/usr/bin/env node

const NODE_ENV = process.env.NODE_ENV;

if ( !NODE_ENV ) {
    throw new Error(`[builder WhatsIsNew] NODE_ENV must be "development" or "production", but not "${NODE_ENV}".`)
}

console.info( `> [WhatsIsNewBuilder env] ${ NODE_ENV }` );

const { build } = require( '../index' );

build()
    .catch( console.log );
