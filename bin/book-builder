#!/usr/bin/env node

const path = require( 'path' );

const NODE_ENV = process.env.NODE_ENV;

if ( !NODE_ENV ) {
    throw new Error(`[book-builder] NODE_ENV must be "development" or "production", but not "${NODE_ENV}".`)
}

console.info( `> [book-builder env] ${ NODE_ENV }` );


const { run } = require( '../index' );

run().catch(console.error);
