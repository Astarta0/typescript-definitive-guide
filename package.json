{
    "name": "typescript-definitive-guide",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "upgrade": "npx npm-upgrade",
        "update-book-contents": "./get-last-version-book-contents-from-github.sh",
        "serve": "npm run update-book-contents && vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "tslint -c tslint.json 'src/**/*.ts'",
        "prettier": "prettier --write 'src/**/*.ts'",
        "register-service-worker": "^1.0.0",
        "test:e2e": "vue-cli-service test:e2e",
        "test:unit": "vue-cli-service test:unit",
        "vue-lint": "vue-cli-service lint"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run prettier",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './config/git/commitlint.js'"
        }
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-customizable"
        },
        "cz-customizable": {
            "config": "config/git/commitizen.js"
        }
    },
    "dependencies": {
        "material-components-web": "^2.2.0",
        "register-service-worker": "^1.6.2",
        "vue": "^2.6.10",
        "vue-material": "^1.0.0-beta-11",
        "vue-mdc-adapter": "^0.18.2",
        "vue-property-decorator": "^8.1.1",
        "vue-router": "^3.0.6",
        "vuex": "^3.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.4.4",
        "@commitlint/cli": "^7.6.1",
        "@types/jest": "^24.0.13",
        "@types/node": "^12.0.2",
        "@vue/cli-plugin-e2e-cypress": "^3.7.0",
        "@vue/cli-plugin-pwa": "^3.7.0",
        "@vue/cli-plugin-typescript": "^3.7.0",
        "@vue/cli-plugin-unit-jest": "^3.7.0",
        "@vue/cli-service": "^3.7.0",
        "@vue/test-utils": "^1.0.0-beta.29",
        "babel-loader": "^8.0.6",
        "babel-preset-vue": "^2.0.2",
        "commitizen": "^3.1.1",
        "cz-customizable": "^6.2.0",
        "html-loader": "^0.5.5",
        "husky": "^2.3.0",
        "node-sass": "^4.12.0",
        "prettier": "^1.17.1",
        "sass-loader": "^7.1.0",
        "svgo": "^1.2.2",
        "svgo-loader": "^2.2.0",
        "translit-rus-eng": "^1.0.7",
        "ts-jest": "^24.0.2",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.4.5",
        "vue-cli-plugin-material": "^0.2.3",
        "vue-cli-plugin-svg-sprite": "^1.0.0",
        "vue-template-compiler": "^2.6.10",
        "webpack-merge": "^4.2.1"
    }
}
