{
  "name": "typescript-definitive-guide",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prettier": "prettier --write 'src/**/*.ts'",
    "register-service-worker": "^1.0.0",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "vue-lint": "vue-cli-service lint"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run prettier",
      "commit-msg": "npm run prettier && commitlint -E HUSKY_GIT_PARAMS -g './config/git/commitlint.js'"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/git/commitizen.js"
    }
  },
  "dependencies": {
    "material-components-web": "^0.35.0",
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.17",
    "vue-material": "^1.0.0-beta-10.2",
    "vue-mdc-adapter": "^0.15.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@commitlint/cli": "^7.2.1",
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-e2e-cypress": "^3.1.2",
    "@vue/cli-plugin-pwa": "^3.1.2",
    "@vue/cli-plugin-typescript": "^3.1.1",
    "@vue/cli-plugin-unit-jest": "^3.1.1",
    "@vue/cli-service": "^3.1.4",
    "@vue/test-utils": "^1.0.0-beta.26",
    "babel-loader": "^8.0.4",
    "babel-preset-vue": "^2.0.2",
    "commitizen": "^3.0.5",
    "cz-customizable": "^5.3.0",
    "html-loader": "^0.5.5",
    "husky": "^1.2.1",
    "node-sass": "^4.10.0",
    "prettier": "^1.15.3",
    "sass-loader": "^7.1.0",
    "svgo": "^1.1.0",
    "svgo-loader": "^2.1.0",
    "translit-rus-eng": "^1.0.7",
    "ts-jest": "^23.0.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.0.0",
    "vue-cli-plugin-material": "^0.2.2",
    "vue-cli-plugin-svg-sprite": "^0.3.1",
    "vue-template-compiler": "^2.5.17",
    "webpack-merge": "^4.1.4"
  }
}
